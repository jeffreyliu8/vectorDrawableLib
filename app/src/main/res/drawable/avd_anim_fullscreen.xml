<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:name="group"
                android:pivotX="12"
                android:pivotY="12">
                <path
                    android:name="path"
                    android:pathData="M 5 14 L 5 19 L 10 19 L 10 17 L 7 17 L 7 14 L 7 14 L 5.149 14 L 5 14 M 7 10 L 7 7 L 10 7 L 10 5 L 5 5 L 5 5 L 5 10 L 5 10 L 7 10 M 14 17 L 14 19 L 19 19 L 19 14 L 17 14 L 17 17 L 17 17 L 14 17 L 14 17 M 19 5 L 14 5 L 14 5 L 14 7 L 17 7 L 17 10 L 19 10 L 19 5 L 19 5"
                    android:fillColor="#A1887F"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="pathData"
                    android:duration="1000"
                    android:valueFrom="M 5 14 L 5 19 L 10 19 L 10 17 L 7 17 L 7 14 L 7 14 L 5.149 14 L 5 14 M 7 10 L 7 7 L 10 7 L 10 5 L 5 5 L 5 5 L 5 10 L 5 10 L 7 10 M 14 17 L 14 19 L 19 19 L 19 14 L 17 14 L 17 17 L 17 17 L 14 17 L 14 17 M 19 5 L 14 5 L 14 5 L 14 7 L 17 7 L 17 10 L 19 10 L 19 5 L 19 5"
                    android:valueTo="M 5 16 L 8 16 L 8 19 L 10 19 L 10 14 L 5 14 L 5 14 L 5 16 L 5 16 M 5 10 L 10 10 L 10 5 L 8 5 L 8 8 L 8 8 L 5 8 L 5 8.108 L 5 10 M 14 19 L 16 19 L 16 16 L 19 16 L 19 14 L 14 14 L 14 14.027 L 14 19 L 14 19 M 16 8 L 16 5 L 16 5 L 14 5 L 14 10 L 19 10 L 19 8 L 16 8 L 16 8"
                    android:valueType="pathType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:duration="300"
                    android:valueFrom="#A1887F"
                    android:valueTo="#388E3C"
                    android:valueType="colorType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="rotation"
                android:duration="1000"
                android:valueFrom="0"
                android:valueTo="90"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
</animated-vector>
